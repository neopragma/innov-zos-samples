CREATE TABLE LABSCHEMA.TEAMS
(
    TEAM_ID    INTEGER GENERATED ALWAYS AS IDENTITY
                 (START WITH 1, INCREMENT BY 1, NO CACHE, NO CYCLE),
    TEAM_NAME  VARCHAR(100) NOT NULL,
    CONSTRAINT PK_TEAMS PRIMARY KEY (TEAM_ID)

)
IN DBMATE1.TSMATExx;  -- Replace with your actual database and tablespace

